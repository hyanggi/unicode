#!/usr/bin/env bash

# This script outputs some Unicode strings which require complex text rendering.

# Combining characters (Marks)
echo 'ÇšuÌˆÌŒ' # Nonspacing mark (Latin)
echo 'à¸­à¸±' # Nonspacing mark (Thai)
echo 'à¤•à¤¿' # Spacing mark (Devanagari)
echo 'à®•à¯†' # Spacing mark (Tamil)
echo 'à®•à¯Š' # Spacing mark (Tamil)
echo 'å­—âƒ' # Enclosing mark
echo 'nÍ g' # Double diacritic nonspacing mark

# Other characters that combine
echo 'í•œá„’á…¡á†«' # Conjoining Hangul Jamo
echo 'Ë¨Ë©Ë¦' # Tone marks (Tone 214)

# Other complex text
# Arabic and Hebrew text is written from right to left
echo 'Ø®Ø®Ø®' # Arabic letters are connected and take contextual forms
echo 'Ù„Ø§' # Arabic letters form ligatures
echo 'á µá  á ·á ¢á ­ á µá ¡á ·á ¢á ­' # Mongolian letters take different forms according to the gender of the word (which depends on the vowels in the word) (However the differing vowel letter itself is displayed identically)
echo 'à¤•à¥à¤·' # Devanagari letters form ligatures
echo 'fi' # Latin letters may form ligatures or be cursively connected depending on the font
echo '!=' # Programming symbols may form ligatures (in this case 'â‰ ') when using a font with programming ligatures

# Emoji
echo '3âƒ£ 3ï¸âƒ£' # Enclosing Keycap
echo 'ğŸ‘ŒğŸ»' # Emoji skin color modifier
echo 'ğŸ‡ºğŸ‡³' # Regional indicator symbols
echo 'ğŸ´ó §ó ¢ó ¥ó ®ó §ó ¿' # Tags
